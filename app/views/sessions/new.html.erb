<div>
    <h1>Login</h1>
    <%= form_for :session, url: login_path, method: :post do |form| %>
      <%= form.label :email, "Email:" %><%= form.email_field :email%><br />
      <%= form.label :password, "Senha:"%><%= form.password_field :password %><br />
      <%= form.submit 'Logar' %>
    <% end %>
</div>